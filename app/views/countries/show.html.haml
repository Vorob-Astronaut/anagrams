.row
  .col-md-3
    =render 'layouts/partials/sidebar'
  .col-md-9.back-ground
    - if @country.is_active? && @country.titles.any?
      %p
        %span
          = @country.country
          .row
            .col-md-12
              - (@country.titles-Title.in_removals(current_user)).each do |title|
                .col-md-3.padding-5{id: "title-#{title.id}"}
                  =render 'shared/title', title: title

