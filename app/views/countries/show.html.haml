.row
  .col-md-2
    =render 'layouts/partials/sidebar'
  %ol.breadcrumb
    = render_breadcrumbs tag: :li, separator: ""
  .col-md-9.back-ground
    - if @country.is_active? && @country.titles.any?
      %p
        %span.results
          = @country.country
          .row
            .col-md-12
              =render @titles
              #results

=pageless(@titles.total_pages, country_path(@country), '#results')
